{
	"name": "vscode-buttons",
	"displayName": "vscode-buttons",
	"description": "Create your own buttons!",
	"publisher": "stuartthomson",
	"author": {
		"name": "Stuart Thomson"
	},
	"license": "MIT",
	"keywords": [
		"shell",
		"terminal",
		"buttons"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/stuartthomson/vscode-buttons.git"
	},
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:buttons",
		"onCommand:vscode-buttons:runCommand"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "buttons",
					"name": "Buttons"
				}
			]
		},
		"commands": [
			{
				"command": "vscode-buttons.runCommand",
				"title": "Run a command...",
				"category": "Buttons"
			}
		],
		"jsonValidation": [
			{
				"fileMatch": "**/buttons.jsonc",
				"url": "./resources/buttonsSchema.json"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "vscode-buttons.runCommand",
					"when": "false"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^10.2.0",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"jsonc-parser": "^2.2.0"
	}
}
